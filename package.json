{
  "name": "beer-advent-calendar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "start:json-server": "json-server --watch --port 5001 ./src/assets/db.json",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "git push heroku master",
    "start:prod": "concurrently \"node server.js\" \"npm run start:json-server\"",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "json-server": "^0.16.3",
    "vue": "^3.0.0",
    "vuex": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "less": "^3.12.2",
    "less-loader": "^7.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
